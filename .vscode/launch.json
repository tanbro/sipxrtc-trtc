{
    // 使用 IntelliSense 了解相关属性。 
    // 悬停以查看现有属性的描述。
    // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "[Linux-x86_64-GNU/Debug] 启动 sipxtrtc",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/Linux-x86_64-GNU/Debug/bin/sipxtrtc",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/out/Linux-x86_64-GNU/Debug/bin",
            "environment": [
                {
                    "name": "GLOG_logtostderr",
                    "value": "1"
                },
                {
                    "name": "GLOG_v",
                    "value": "3"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}